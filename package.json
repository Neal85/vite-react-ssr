{
  "name": "vite-react-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.backend.json --clear-screen=false --ignore **/vite.config.ts* ./backend/server.ts",
    "build": "npm run build:client && npm run build:ssr && npm run build:backend",
    "build:client": "tsc && vite build",
    "build:ssr": "node scripts/build.js ssr",
    "build:backend": "tsc -p tsconfig.backend.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cross-env NODE_ENV=production node ./dist_backend/backend/server.js"
  },
  "dependencies": {
    "@maxmind/geoip2-node": "^4.2.0",
    "body-parser": "^1.20.2",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "response-time": "^2.3.2",
    "sass": "^1.69.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.4",
    "@types/client-sessions": "^0.8.5",
    "@types/cookie-parser": "^1.4.5",
    "@types/ejs": "^3.1.4",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/response-time": "^2.3.6",
    "@types/uuid": "^9.0.5",
    "@types/yargs": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tsx": "^3.14.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
